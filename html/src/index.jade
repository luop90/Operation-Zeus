html(lang="en" ng-app="zeus")
  head
    meta(charset="utf-8")
    title Operation Zeus
    link(rel="stylesheet" href="../../assets/build/css/master.min.css" media="screen" title="no title" charset="utf-8")
  body(ng-controller="MainCtrl" ng-class="{ 'dark-theme': $root.darkTheme }")
    .navbar-fixed
      nav
        .nav-wrapper
          a(href="#/").brand-logo.center Operation Zeus
          ul.left
            li.waves-effect.waves-light
              a(href="#/")
                i.material-icons settings_input_antenna
          ul.right
            li.waves-effect.waves-light
              a(href="#/settings")
                i.material-icons settings
    main
      .container(ng-view)
    
    script.
      if (typeof module === 'object') {window.module = module; module = undefined;}
    script(src="../../assets/build/js/jquery.min.js", charset="utf-8")
    script.
      if (window.module) module = window.module;
    script(src="../../assets/build/js/materialize.min.js" charset="utf-8")
    script(src="../../assets/build/js/angular.min.js" charset="utf-8")
    script(src="../../assets/build/js/angular-route.min.js" charset="utf-8")
    script(src="../../assets/build/js/operation-zeus.min.js" charset="utf-8")
    script(src="../../zeus.js" charset="utf-8")
